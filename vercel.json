{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "env": {
                "PRISMA_GENERATE_CMD": "prisma generate"
            },
            "functions": {
                "preBuild": {
                    "command": "$PRISMA_GENERATE_CMD"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}