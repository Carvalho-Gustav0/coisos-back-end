// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model User {
  id_user    String @id
  user_token String
  name       String
  cpf        String
  email      String
  password   String

  @@index([id_user])
}
